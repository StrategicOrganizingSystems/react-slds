{
  "name": "react-slds",
  "version": "0.1.3",
  "description": "Reusable React library of Salesforce Lightning Design System (SLDS) components.",
  "main": "lib/index.js",
  "scripts": {
    "storybook": "start-storybook -p 9001 -c .storybook -s ./node_modules/@salesforce-ux/design-system,./public",
    "test": "react-scripts test --env=jsdom --testPathPattern ./src/.*",
    "compile": "rm -rf ./lib && babel --loose es6.modules -d ./lib/ ./src/ --ignore test.js,setupTests.js",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "lint:eslint": "eslint --fix --cache --cache-location .eslintcache --ignore-path .eslintignore --config .eslintrc src stories",
    "tests": "babel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cubiccompass/react-slds.git"
  },
  "author": "Mike Leach <mike@cubiccompass.com>",
  "contributors": [
    {
      "name": "Jeric Bas",
      "email": "jeric@cubiccompass.com"
    },
    {
      "name": "Julius Calahi",
      "email": "julius@cubiccompass.com"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cubiccompass/react-slds/issues"
  },
  "homepage": "https://github.com/cubiccompass/react-slds#readme",
  "dependencies": {
    "@salesforce-ux/design-system": "^2.6.2",
    "@storybook/cli": "^3.3.13",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.5",
    "date-fns": "^1.29.0",
    "eslint": "^5.12.0",
    "global": "^4.3.2",
    "lodash.filter": "^4.6.0",
    "lodash.includes": "^4.3.0",
    "lodash.map": "^4.6.0",
    "lodash.max": "^4.0.1",
    "lodash.orderby": "^4.6.0",
    "lodash.pickby": "^4.6.0",
    "lodash.remove": "^4.7.0",
    "lodash.sumby": "^4.6.0",
    "lodash.uniqueid": "^4.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "svg4everybody": "^2.1.9"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.11",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/addon-info": "^3.3.11",
    "@storybook/addon-knobs": "^3.3.11",
    "@storybook/addon-links": "^3.3.11",
    "@storybook/addon-notes": "^3.3.11",
    "@storybook/addon-options": "^3.3.11",
    "@storybook/addon-storyshots": "^3.3.10",
    "@storybook/addons": "^3.3.11",
    "@storybook/react": "^3.3.10",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.7.0",
    "babel-eslint": "^6.0.2",
    "babel-istanbul": "^0.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.3.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.12.3",
    "husky": "^1.1.1",
    "jest-cli": "^20.0.4",
    "lint-staged": "^8.0.4",
    "path-exists-cli": "^1.0.0",
    "react-scripts": "^1.0.17",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "storybook-addon-jsx": "^5.3.0",
    "storybook-addon-smart-knobs": "^3.3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  }
}
